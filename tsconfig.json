{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@ui/*": [
        "src/components/ui/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@actions/*": [
        "src/actions/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@assets/svgs/*": [
        "src/assets/svgs/*"
      ],
      "@utils/*": [
        "src/lib/utils/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}